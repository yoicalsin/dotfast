{
   "name": "dotfast",
   "version": "1.0.3",
   "description": "Dotfast helps to play with the keys nested in the objects, in a very easy way, and with very little code !",
   "main": "index.js",
   "type": "index.d.ts",
   "scripts": {
      "build": "yarn prebuild && tsc",
      "start": "node index.js",
      "dev": "yarn prebuild && tsc -w",
      "dev:start": "nodemon index.js",
      "prebuild": "rimraf index.js && rimraf index.d.ts",
      "test": "jest"
   },
   "repository": {
      "type": "git",
      "url": "git+https://github.com/yonicalsin/dotfast.git"
   },
   "keywords": [
      "format",
      "string-replace",
      "replaces",
      "string",
      "dotfast",
      "dottie",
      "dot-objects"
   ],
   "author": "Yoni Calsin <helloyonicb@gmail.com>",
   "license": "MIT",
   "bugs": {
      "url": "https://github.com/yonicalsin/dotfast/issues"
   },
   "homepage": "https://github.com/yonicalsin/dotfast#readme",
   "devDependencies": {
      "nodemon": "^2.0.3",
      "prettier-plugin-packagejson": "^2.2.3",
      "rimraf": "^3.0.2",
      "@types/jest": "^24.0.25",
      "jest": "^24.9.0",
      "ts-jest": "^24.2.0"
   },
   "dependencies": {
      "is-all-utils": "^1.0.4"
   }
}
